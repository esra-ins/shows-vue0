{"ast":null,"code":"// @ is an alias to /src\n// import HelloWorld from '@/components/HelloWorld.vue'\n// import HeaderCompVue\n// import HeaderCompVue from '@/components/HeaderComp.vue'\n// import HeaderCompVue from '@/components/HeaderComp.vue'\nimport { ref } from 'vue';\nimport env from '@/env.js';\nexport default {\n  name: 'HomeView',\n  components: {},\n  setup() {\n    const search = ref(\"\");\n    //const url = ref(\"\");\n    const movies = ref([]);\n    const topRatedShows = ref([]);\n    const popularShows = ref([]);\n    const imgBaseUrl = env.imgBaseUrl;\n    const SearchMovies = () => {\n      if (search.value != \"\") {\n        console.log(search.value);\n\n        //console.log(`${env.baseUrl}search/movie?${env.apikey}&query=${search.value}`);\n        const searchUrl = `${env.baseUrl}search/movie?${env.apikey}&query=${search.value}`;\n        //const imgUrl=`${env.imgBaseUrl}`;\n        console.log(searchUrl);\n        //console.log( url  );\n\n        fetch(searchUrl).then(response => response.json()).then(data => {\n          console.log(data);\n          movies.value = data.results; //array name: results\n          //data.results.\n          search.value = \"\";\n          console.log(movies.value);\n        });\n      }\n    };\n\n    /*\n    return {\n      search,\n      movies,\n      SearchMovies,\n      imgUrl\n    }\n    */\n\n    const GetTopRatedShows = () => {\n      const topRatedShowsUrl = `${env.baseUrl}tv/top_rated?${env.apikey}`;\n      fetch(topRatedShowsUrl).then(response => response.json()).then(data => {\n        topRatedShows.value = data.results;\n\n        //console.log(topRatedShows.value);\n      });\n    };\n\n    const GetPopularShows = () => {\n      const popularShowsUrl = `${env.baseUrl}tv/popular?${env.apikey}`;\n      fetch(popularShowsUrl).then(response => response.json()).then(data => {\n        popularShows.value = data.results;\n\n        //console.log(popularShows.value);\n      });\n    };\n\n    return {\n      search,\n      movies,\n      SearchMovies,\n      GetTopRatedShows,\n      topRatedShows,\n      imgBaseUrl,\n      GetPopularShows,\n      popularShows\n    };\n  }\n};","map":{"version":3,"names":["ref","env","name","components","setup","search","movies","topRatedShows","popularShows","imgBaseUrl","SearchMovies","value","console","log","searchUrl","baseUrl","apikey","fetch","then","response","json","data","results","GetTopRatedShows","topRatedShowsUrl","GetPopularShows","popularShowsUrl"],"sources":["/home/esraisik/Desktop/all-e/my-vue/theshow-vue/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <!-- <img alt=\"Vue logo\" src=\"../assets/logo.png\"> -->\n    <!-- <HelloWorld msg=\"Welcome to Your Vue.js App\"/> -->\n     <!-- <header-comp-vue/> -->\n  </div>\n   <form @submit.prevent=\"SearchMovies()\">\n    <input type=\"text\" placeholder=\"arayın\"  v-model=\"search\"/>  \n    <input type=\"submit\" value=\"Search\"/>\n  </form>\n\n  <div class=\"movies-list\">\n    <div class=\"movie\" v-for=\"movie in movies\" :key=\"movie.id\">\n      {{movie.title}}\n      {{movie.vote_average}}\n        <img :src=\"imgBaseUrl+movie.poster_path\" />\n\n      <!-- <img :src=\"'${env.imgBaseUrl}'+movie.poster_path\"/>  -->\n      <!-- <img :src=\"imgUrl+'{{movie.poster_path}}'\"/> -->\n    </div>\n    <br/>\n    <hr/>\n\n      <!-- <input type=\"button\" value=\"TOP RATED MOVİES\" @click=\"GetTopRatedMovies\"> -->\n\n    <div class=\"top-rated-list\">\n      <input type=\"button\" value=\"TOP RATED TV-SHOWS\" @click=\"GetTopRatedShows()\">\n      <div class=\"top-rated\" v-for=\"show in topRatedShows\" :key=\"show.id\" >\n          {{show.name}}\n          {{show.vote_average}}\n        <img :src=\"imgBaseUrl+show.poster_path\" />\n      </div>\n    </div>\n\n    <div class=\"popular-list\">\n      <input type=\"button\" value=\"POPULAR TV-SHOWS\" @click=\"GetPopularShows()\">\n      <div class=\"top-rated\" v-for=\"show in popularShows\" :key=\"show.id\" >\n          {{show.name}}\n          {{show.vote_average}}\n        <img :src=\"imgBaseUrl+show.poster_path\" />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n// import HelloWorld from '@/components/HelloWorld.vue'\n// import HeaderCompVue\n// import HeaderCompVue from '@/components/HeaderComp.vue'\n// import HeaderCompVue from '@/components/HeaderComp.vue'\nimport {ref} from 'vue';\nimport env from '@/env.js';\n\nexport default {\n  name: 'HomeView',\n  components: {\n    \n  },\n  setup () {\n    const search = ref(\"\");\n    //const url = ref(\"\");\n    const movies = ref([]);\n    const topRatedShows = ref([]);\n    const popularShows = ref([]);\n    const imgBaseUrl = env.imgBaseUrl;\n\n\n    const SearchMovies = () => {\n      if (search.value!=\"\") {\n        console.log(search.value);\n        \n        //console.log(`${env.baseUrl}search/movie?${env.apikey}&query=${search.value}`);\n        const searchUrl=`${env.baseUrl}search/movie?${env.apikey}&query=${search.value}`;\n        //const imgUrl=`${env.imgBaseUrl}`;\n        console.log(searchUrl);\n        //console.log( url  );\n        \n        fetch(searchUrl).then(response => response.json()).then(data => {\n          console.log(data);\n\n          movies.value = data.results;  //array name: results\n          //data.results.\n          search.value = \"\";\n          console.log(movies.value);\n        });\n      }\n    }\n\n    /*\n    return {\n      search,\n      movies,\n      SearchMovies,\n      imgUrl\n    }\n    */\n     \n     const GetTopRatedShows = () => {\n      const topRatedShowsUrl=`${env.baseUrl}tv/top_rated?${env.apikey}`;\n\n      fetch(topRatedShowsUrl).then(response => response.json()).then(data => {\n          topRatedShows.value=data.results;\n\n          //console.log(topRatedShows.value);\n      })\n     }\n\n\n     const GetPopularShows=()=>{\n      const popularShowsUrl=`${env.baseUrl}tv/popular?${env.apikey}`;\n      fetch(popularShowsUrl).then(response=>response.json()).then(data=>{\n          popularShows.value=data.results;\n\n          //console.log(popularShows.value);\n      })\n     }\n\n\n     return {\n      search,\n      movies,\n      SearchMovies,\n      GetTopRatedShows,\n      topRatedShows,\n      imgBaseUrl,\n      GetPopularShows,\n      popularShows\n    }\n  }\n}\n</script>\n"],"mappings":"AA8CA;AACA;AACA;AACA;AACA;AACA,SAAQA,GAAG,QAAO,KAAK;AACvB,OAAOC,GAAE,MAAO,UAAU;AAE1B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE,CAEZ,CAAC;EACDC,KAAIA,CAAA,EAAK;IACP,MAAMC,MAAK,GAAIL,GAAG,CAAC,EAAE,CAAC;IACtB;IACA,MAAMM,MAAK,GAAIN,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMO,aAAY,GAAIP,GAAG,CAAC,EAAE,CAAC;IAC7B,MAAMQ,YAAW,GAAIR,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMS,UAAS,GAAIR,GAAG,CAACQ,UAAU;IAGjC,MAAMC,YAAW,GAAIA,CAAA,KAAM;MACzB,IAAIL,MAAM,CAACM,KAAK,IAAE,EAAE,EAAE;QACpBC,OAAO,CAACC,GAAG,CAACR,MAAM,CAACM,KAAK,CAAC;;QAEzB;QACA,MAAMG,SAAS,GAAE,GAAEb,GAAG,CAACc,OAAQ,gBAAed,GAAG,CAACe,MAAO,UAASX,MAAM,CAACM,KAAM,EAAC;QAChF;QACAC,OAAO,CAACC,GAAG,CAACC,SAAS,CAAC;QACtB;;QAEAG,KAAK,CAACH,SAAS,CAAC,CAACI,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,EAAE,CAAC,CAACF,IAAI,CAACG,IAAG,IAAK;UAC9DT,OAAO,CAACC,GAAG,CAACQ,IAAI,CAAC;UAEjBf,MAAM,CAACK,KAAI,GAAIU,IAAI,CAACC,OAAO,EAAG;UAC9B;UACAjB,MAAM,CAACM,KAAI,GAAI,EAAE;UACjBC,OAAO,CAACC,GAAG,CAACP,MAAM,CAACK,KAAK,CAAC;QAC3B,CAAC,CAAC;MACJ;IACF;;IAEA;;;;;;;;;IASC,MAAMY,gBAAe,GAAIA,CAAA,KAAM;MAC9B,MAAMC,gBAAgB,GAAE,GAAEvB,GAAG,CAACc,OAAQ,gBAAed,GAAG,CAACe,MAAO,EAAC;MAEjEC,KAAK,CAACO,gBAAgB,CAAC,CAACN,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,EAAE,CAAC,CAACF,IAAI,CAACG,IAAG,IAAK;QACnEd,aAAa,CAACI,KAAK,GAACU,IAAI,CAACC,OAAO;;QAEhC;MACJ,CAAC;IACF;;IAGA,MAAMG,eAAe,GAACA,CAAA,KAAI;MACzB,MAAMC,eAAe,GAAE,GAAEzB,GAAG,CAACc,OAAQ,cAAad,GAAG,CAACe,MAAO,EAAC;MAC9DC,KAAK,CAACS,eAAe,CAAC,CAACR,IAAI,CAACC,QAAQ,IAAEA,QAAQ,CAACC,IAAI,EAAE,CAAC,CAACF,IAAI,CAACG,IAAI,IAAE;QAC9Db,YAAY,CAACG,KAAK,GAACU,IAAI,CAACC,OAAO;;QAE/B;MACJ,CAAC;IACF;;IAGA,OAAO;MACNjB,MAAM;MACNC,MAAM;MACNI,YA