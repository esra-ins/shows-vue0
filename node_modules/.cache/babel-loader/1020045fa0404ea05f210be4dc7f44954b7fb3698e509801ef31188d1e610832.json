{"ast":null,"code":"// @ is an alias to /src\n//import HelloWorld from '@/components/HelloWorld.vue'\n//import HeaderCompVue\n// import HeaderCompVue from '@/components/HeaderComp.vue'\n//import HeaderCompVue from '@/components/HeaderComp.vue'\nimport { ref } from 'vue';\nimport env from '@/env.js';\nexport default {\n  name: 'HomeView',\n  components: {},\n  setup() {\n    const search = ref(\"\");\n    //const url=ref(\"\");\n    const movies = ref([]);\n    const topRatedMovies = ref([]);\n    const imgBaseUrl = env.imgBaseUrl;\n    const SearchMovies = () => {\n      if (search.value != \"\") {\n        console.log(search.value);\n\n        //console.log(`${env.baseUrl}search/movie?${env.apikey}&query=${search.value}`);\n        const searchUrl = `${env.baseUrl}search/movie?${env.apikey}&query=${search.value}`;\n        //const imgUrl=`${env.imgBaseUrl}`;\n        console.log(searchUrl);\n        //console.log( url  );\n        fetch(searchUrl).then(response => response.json()).then(data => {\n          console.log(data);\n          movies.value = data.results; //array name: results\n          //data.results.\n          search.value = \"\";\n          console.log(movies.value);\n        });\n      }\n    };\n\n    // return{\n    // search,\n    // movies,\n    // SearchMovies,\n    //imgUrl\n    // }\n\n    const GetTopRatedMovies = () => {\n      const topRatedMoviesUrl = `${env.baseUrl}movie/top_rated?${env.apikey}`;\n      fetch(topRatedMoviesUrl).then(response => response.json()).then(data => {\n        topRatedMovies.value = data.results;\n        console.log(topRatedMovies.value);\n      });\n    };\n    return {\n      search,\n      movies,\n      SearchMovies,\n      GetTopRatedMovies,\n      topRatedMovies,\n      imgBaseUrl\n    };\n  }\n};","map":{"version":3,"names":["ref","env","name","components","setup","search","movies","topRatedMovies","imgBaseUrl","SearchMovies","value","console","log","searchUrl","baseUrl","apikey","fetch","then","response","json","data","results","GetTopRatedMovies","topRatedMoviesUrl"],"sources":["C:\\Users\\suraes\\Desktop\\new vue\\theshow-vue\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <!-- <img alt=\"Vue logo\" src=\"../assets/logo.png\"> -->\n    <!-- <HelloWorld msg=\"Welcome to Your Vue.js App\"/> -->\n     <!-- <header-comp-vue/> -->\n  </div>\n\n   <form @submit.prevent=\"SearchMovies()\">\n    <input type=\"text\" placeholder=\"arayın\"  v-model=\"search\"/>  //v-model 2 bind\n    <input type=\"submit\" value=\"Search\"/>\n   </form>\n\n   <div class=\"movies-list\">\n    <div class=\"movie\" v-for=\"movie in movies\" :key=\"movie.id\">\n      {{movie.title}}\n        <img :src=\"imgBaseUrl+movie.poster_path\" />\n\n      <!-- <img :src=\"'${env.imgBaseUrl}'+movie.poster_path\"/>  -->\n      <!-- <img :src=\"imgUrl+'{{movie.poster_path}}'\"/> -->\n\n    </div>\n\n    <br/>\n    <hr/>\n\n      <!-- <input type=\"button\" value=\"TOP RATED MOVİES\" @click=\"GetTopRatedMovies\"> -->\n<div>\n    <div class=\"top-rated-list\">\n      <input type=\"button\" value=\"TOP RATED MOVİES\" @click=\"GetTopRatedMovies()\">\n\n      <div class=\"top-rated\" v-for=\"movie in topRatedMovies\" :key=\"movie.id\" >\n          {{movie.title}}\n        <img :src=\"imgBaseUrl+movie.poster_path\" />\n\n      </div>\n    </div>\n\n    <div class=\"top-rated-list\">\n      <input type=\"button\" value=\"POPULAR MOVİES\" @click=\"GetTopRatedMovies()\">\n      <div class=\"top-rated\" v-for=\"movie in topRatedMovies\" :key=\"movie.id\" >\n          {{movie.title}}\n        <img :src=\"imgBaseUrl+movie.poster_path\" />\n      </div>\n    </div>\n</div>\n\n   </div>\n\n\n\n</template>\n\n<script>\n// @ is an alias to /src\n//import HelloWorld from '@/components/HelloWorld.vue'\n//import HeaderCompVue\n// import HeaderCompVue from '@/components/HeaderComp.vue'\n//import HeaderCompVue from '@/components/HeaderComp.vue'\nimport {ref} from 'vue';\nimport env from '@/env.js';\n\nexport default {\n  name: 'HomeView',\n  components: {\n    \n  },\n  setup(){\n    const search=ref(\"\");\n    //const url=ref(\"\");\n    const movies=ref([]);\n    const topRatedMovies=ref([]);\n    const imgBaseUrl= env.imgBaseUrl;\n\n\n    const SearchMovies=()=>{\n      if(search.value!=\"\"){\n        console.log(search.value);\n        \n        //console.log(`${env.baseUrl}search/movie?${env.apikey}&query=${search.value}`);\n        const searchUrl=`${env.baseUrl}search/movie?${env.apikey}&query=${search.value}`;\n        //const imgUrl=`${env.imgBaseUrl}`;\n        console.log(searchUrl);\n        //console.log( url  );\n        fetch(searchUrl).then(response=>response.json()).then(data=>{\n          console.log(data);\n          movies.value=data.results;  //array name: results\n          //data.results.\n          search.value=\"\";\n          console.log(movies.value);\n        });\n\n      }\n    }\n\n    // return{\n      // search,\n      // movies,\n      // SearchMovies,\n      //imgUrl\n    // }\n     \n     const GetTopRatedMovies=()=>{\n      const topRatedMoviesUrl=`${env.baseUrl}movie/top_rated?${env.apikey}`;\n      fetch(topRatedMoviesUrl).then(response=>response.json()).then(data=>{\n          topRatedMovies.value=data.results;\n          console.log(topRatedMovies.value);\n      })\n     }\n\n     return{\n      search,\n      movies,\n      SearchMovies,\n      GetTopRatedMovies,\n      topRatedMovies,\n      imgBaseUrl\n     }\n  }\n\n\n}\n</script>\n"],"mappings":"AAqDA;AACA;AACA;AACA;AACA;AACA,SAAQA,GAAG,QAAO,KAAK;AACvB,OAAOC,GAAE,MAAO,UAAU;AAE1B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE,CAEZ,CAAC;EACDC,KAAKA,CAAA,EAAE;IACL,MAAMC,MAAM,GAACL,GAAG,CAAC,EAAE,CAAC;IACpB;IACA,MAAMM,MAAM,GAACN,GAAG,CAAC,EAAE,CAAC;IACpB,MAAMO,cAAc,GAACP,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMQ,UAAU,GAAEP,GAAG,CAACO,UAAU;IAGhC,MAAMC,YAAY,GAACA,CAAA,KAAI;MACrB,IAAGJ,MAAM,CAACK,KAAK,IAAE,EAAE,EAAC;QAClBC,OAAO,CAACC,GAAG,CAACP,MAAM,CAACK,KAAK,CAAC;;QAEzB;Q