{"ast":null,"code":"import { ref } from 'vue';\nimport env from '@/env.js';\nexport default {\n  name: 'HomeView',\n  components: {},\n  /*\n  data() {\n      return {\n        search: \"\",\n        awaitingSearch: false,\n      };\n    },\n    */\n\n  setup() {\n    const search = ref(\"\");\n    const movies = ref([]);\n    const topRatedShows = ref([]);\n    const popularShows = ref([]);\n    const imgBaseUrl = env.imgBaseUrl;\n    let awaitingSearch = false;\n    const GetTopRatedShows = () => {\n      const topRatedShowsUrl = `${env.baseUrl}tv/top_rated?${env.apikey}`;\n      fetch(topRatedShowsUrl).then(response => response.json()).then(data => {\n        topRatedShows.value = data.results;\n\n        //console.log(topRatedShows.value);\n      });\n    };\n\n    const GetPopularShows = () => {\n      const popularShowsUrl = `${env.baseUrl}tv/popular?${env.apikey}`;\n      fetch(popularShowsUrl).then(response => response.json()).then(data => {\n        popularShows.value = data.results;\n\n        //console.log(popularShows.value);\n      });\n    };\n\n    const SearchMovies = _.debounce(() => {\n      if (search.value && search.value.length >= 3) {\n        console.log(search.value);\n        const searchUrl = `${env.baseUrl}search/movie?${env.apikey}&query=${search.value}`;\n        console.log(searchUrl);\n        fetch(searchUrl).then(response => response.json()).then(data => {\n          console.log(data);\n          movies.value = data.results; //array name: results\n\n          search.value = \"\";\n          console.log(movies.value);\n        });\n      }\n    }, 3000);\n\n    /*\n    if (!awaitingSearch) {\n          setTimeout(() => {\n            SearchMovies;\n             awaitingSearch = false;\n          }, 2000); // 1 sec delay\n        }\n    awaitingSearch = true;\n    */\n\n    return {\n      search,\n      movies,\n      SearchMovies,\n      GetTopRatedShows,\n      topRatedShows,\n      imgBaseUrl,\n      GetPopularShows,\n      popularShows\n    };\n  }\n\n  /*\n  watch: {\n      search: function () {\n        if (!this.awaitingSearch) {\n          setTimeout(() => {\n            //this.fetchResults({ query: this.search });\n            SearchMovies;\n            this.awaitingSearch = false;\n          }, 2000); // 1 sec delay\n        }\n        this.awaitingSearch = true;\n      },\n    },\n  \n  */\n};","map":{"version":3,"names":["ref","env","name","components","setup","search","movies","topRatedShows","popularShows","imgBaseUrl","awaitingSearch","GetTopRatedShows","topRatedShowsUrl","baseUrl","apikey","fetch","then","response","json","data","value","results","GetPopularShows","popularShowsUrl","SearchMovies","_","debounce","length","console","log","searchUrl"],"sources":["/home/esraisik/Desktop/shows-vue0-main/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"mainbanner bg-cover h-[600px]\">\n    <div class=\"column_wrapper ml-8\">\n      <div class=\"content_wrapper wrap ml-8 text-white font-bold\">\n        <div class=\"title pt-8 pb-16\">\n          <h2 class=\"text-5xl pt-8\">Welcome.</h2>\n          <h3 class=\"text-4xl pt-4\">Millions of movies, TV shows and people to discover. Explore now.</h3>\n        </div>\n        <div class=\"search inline-flex h-[46px] w-4/5\">\n          <!-- <form @submit.prevent=\"SearchMovies()\" class=\"h-[46px] w-4/5 inline-flex\"> \n            v-debounce:400ms\n           -->\n          <form class=\"h-[46px] w-4/5 inline-flex\">\n            <label class=\"h-[46px] w-full rounded-full\">\n                <input class=\"w-full h-full rounded-full pl-2 text-black\"  @keyup=\"SearchMovies()\" v-model=\"search\" label=\"Search\" placeholder=\"Search for a movie, tv show, person......\">\n            </label> \n            <input class=\"rounded-full bg-green w-24 h-[46px] ml-[-40px]\" type=\"submit\" value=\"Search\"/>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <div class=\"list-1\">\n    <div class=\"movie\" v-for=\"movie in movies\" :key=\"movie.id\">\n      {{movie.title}}\n      {{movie.vote_average}}\n      <img :src=\"imgBaseUrl+movie.poster_path\" />\n    </div>\n  </div>\n\n  <div>\n    <br/>\n    <hr/>\n\n    <div class=\"top-rated-list mb-8 font-bold\">\n      <input class=\"cursor-pointer\" type=\"button\" value=\"TOP RATED TV-SHOWS\" @click=\"GetTopRatedShows()\">\n      <div class=\"w-full overflow-hidden\">\n        <div class=\"inline-flex overflow-x-scroll w-full overflow-y-hidden\">\n          <div class=\"top-rated  m-4 w-32 h-60 min-w-fit\" v-for=\"show in topRatedShows\" :key=\"show.id\" >\n            <img :src=\"imgBaseUrl+show.poster_path\" class=\"max-h-40 rounded-xl\"/>\n            <div class=\"w-9 h-9 bg-black text-white text-center rounded-2xl -mt-4 pt-2 border-white relative\">{{show.vote_average}}</div>\n              <div class=\"w-min mt-1 min-w-fit\">{{show.name}}</div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"popular-list font-bold mb-12\">\n      <input class=\"cursor-pointer mb-2\" type=\"button\" value=\"POPULAR TV-SHOWS\" @click=\"GetPopularShows()\">\n      <!--  <input class=\"cursor-pointer mb-2\" type=\"button\" value=\"POPULAR TV-SHOWS\" @click=\"GetPopularShows()\">  -->\n      <div class=\"w-full overflow-hidden\">\n        <div class=\"inline-flex overflow-x-scroll w-full overflow-y-hidden\">\n          <div class=\"top-rated m-4 w-32 h-60 min-w-fit\" v-for=\"show in popularShows\" :key=\"show.id\" >\n            <img :src=\"imgBaseUrl+show.poster_path\" class=\"max-h-40 rounded-xl\"/>\n            <div class=\"w-9 h-9 bg-black text-white text-center rounded-2xl -mt-4 pt-2 border-white relative\">{{show.vote_average}}</div>\n            <div class=\"w-min mt-1 min-w-fit\">{{show.name}}</div>\n          </div>\n        </div>\n      </div>\n    </div>\n  \n  </div>\n\n</template>\n\n<script>\nimport {ref} from 'vue';\nimport env from '@/env.js';\n\nexport default {\n  name: 'HomeView',\n  components: {\n    \n  },\n  /*\n  data() {\n      return {\n        search: \"\",\n        awaitingSearch: false,\n      };\n    },\n    */\n    \n\n  setup () {\n    const search = ref(\"\");\n    const movies = ref([]);\n    const topRatedShows = ref([]);\n    const popularShows = ref([]);\n    const imgBaseUrl = env.imgBaseUrl;\n\n    let awaitingSearch = false;\n     \n    const GetTopRatedShows = () => {\n      const topRatedShowsUrl = `${env.baseUrl}tv/top_rated?${env.apikey}`;\n\n      fetch(topRatedShowsUrl).then(response => response.json()).then(data => {\n          topRatedShows.value=data.results;\n\n          //console.log(topRatedShows.value);\n      })\n    }\n\n    const GetPopularShows = () => {\n      const popularShowsUrl = `${env.baseUrl}tv/popular?${env.apikey}`;\n\n      fetch(popularShowsUrl).then(response => response.json()).then(data => {\n          popularShows.value = data.results;\n\n          //console.log(popularShows.value);\n      })\n    }\n\n    const SearchMovies = _.debounce(() => {\n      if (search.value && search.value.length >= 3){\n        console.log(search.value);\n\n        const searchUrl=`${env.baseUrl}search/movie?${env.apikey}&query=${search.value}`;\n\n        console.log(searchUrl);\n\n        fetch(searchUrl).then(response => response.json()).then(data => {\n          console.log(data);\n\n          movies.value = data.results;  //array name: results\n\n          search.value = \"\";\n\n          console.log(movies.value);\n        });\n      }\n    }, 3000)\n\n    /*\n    if (!awaitingSearch) {\n          setTimeout(() => {\n            SearchMovies;\n\n            awaitingSearch = false;\n          }, 2000); // 1 sec delay\n        }\n    awaitingSearch = true;\n    */\n\n    return {\n      search,\n      movies,\n      SearchMovies,\n      GetTopRatedShows,\n      topRatedShows,\n      imgBaseUrl,\n      GetPopularShows,\n      popularShows\n    }\n  },\n\n  /*\n  watch: {\n      search: function () {\n        if (!this.awaitingSearch) {\n          setTimeout(() => {\n            //this.fetchResults({ query: this.search });\n            SearchMovies;\n            this.awaitingSearch = false;\n          }, 2000); // 1 sec delay\n        }\n        this.awaitingSearch = true;\n      },\n    },\n  \n  */\n    \n}\n</script>\n\n<style>\n.mainbanner {\n    background-image: linear-gradient(to right, rgba(6, 57, 112, 0.8) 0%, rgba(6, 57, 112, 0) 100%), url(\"https://www.themoviedb.org/t/p/w1920_and_h800_multi_faces/4anfueQJG7PzfVain87idtuK3hl.jpg\");\n}\n</style>\n"],"mappings":"AAmEA,SAAQA,GAAG,QAAO,KAAK;AACvB,OAAOC,GAAE,MAAO,UAAU;AAE1B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE,CAEZ,CAAC;EACD;;;;;;;;;EAUAC,KAAIA,CAAA,EAAK;IACP,MAAMC,MAAK,GAAIL,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMM,MAAK,GAAIN,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMO,aAAY,GAAIP,GAAG,CAAC,EAAE,CAAC;IAC7B,MAAMQ,YAAW,GAAIR,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMS,UAAS,GAAIR,GAAG,CAACQ,UAAU;IAEjC,IAAIC,cAAa,GAAI,KAAK;IAE1B,MAAMC,gBAAe,GAAIA,CAAA,KAAM;MAC7B,MAAMC,gBAAe,GAAK,GAAEX,GAAG,CAACY,OAAQ,gBAAeZ,GAAG,CAACa,MAAO,EAAC;MAEnEC,KAAK,CAACH,gBAAgB,CAAC,CAACI,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,EAAE,CAAC,CAACF,IAAI,CAACG,IAAG,IAAK;QACnEZ,aAAa,CAACa,KAAK,GAACD,IAAI,CAACE,OAAO;;QAEhC;MACJ,CAAC;IACH;;IAEA,MAAMC,eAAc,GAAIA,CAAA,KAAM;MAC5B,MAAMC,eAAc,GAAK,GAAEtB,GAAG,CAACY,OAAQ,cAAaZ,GAAG,CAACa,MAAO,EAAC;MAEhEC,KAAK,CAACQ,eAAe,CAAC,CAACP,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,EAAE,CAAC,CAACF,IAAI,CAACG,IAAG,IAAK;QAClEX,YAAY,CAACY,KAAI,GAAID,IAAI,CAACE,OAAO;;QAEjC;MACJ,CAAC;IACH;;IAEA,MAAMG,YAAW,GAAIC,CAAC,CAACC,QAAQ,CAAC,MAAM;MACpC,IAAIrB,MAAM,CAACe,KAAI,IAAKf,MAAM,CAACe,KAAK,CAACO,MAAK,IAAK,CAAC,EAAC;QAC3CC,OAAO,CAACC,GAAG,CAACxB,MAAM,CAACe,KAAK,CAAC;QAEzB,MAAMU,SAAS,GAAE,GAAE7B,GAAG,CAACY,OAAQ,gBAAeZ,GAAG,CAACa,MAAO,UAAST,MAAM,CAACe,KAAM,EAAC;QAEhFQ,OAAO,CAACC,GAAG,CAACC,SAAS,CAAC;QAEtBf,KAAK,CAACe,SAAS,CAAC,CAACd,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,EAAE,CAAC,CAACF,IAAI,CAACG,IAAG,IAAK;UAC9DS,OAAO,CAACC,GAAG,CAACV,IAAI,CAAC;UAEjBb,MA